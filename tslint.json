{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "ordered-imports": [
      true,
      {
        "named-imports-order": "lowercase-first"
      }
    ],
    "jsx-no-multiline-js": false,
    "no-var-requires": false
  },
  "rulesDirectory": []
}
